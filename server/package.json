{
  "name": "server",
  "version": "1.0.0",
  "main": "./src/index.js",
  "scripts": {
    "start": "tsx watch ./src/index.ts",
    "start:prod": "tsx ./src/index.ts",
    "start:debug": "tsx --inspect-brk ./src/index.ts",
    "migrate": "tsx scripts/db/migrate.ts",
    "generate:migrations": "npx drizzle-kit generate",
    "generate:migrations:custom": "npx drizzle-kit generate --custom",
    "read:db:csv": "cross-env OUTPUT_FORMAT=csv tsx scripts/db/read.ts",
    "read:db:json": "cross-env OUTPUT_FORMAT=json tsx scripts/db/read.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.43.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.1",
    "postgres": "^3.4.5",
    "request-ip": "^3.3.0",
    "tsx": "^4.20.4",
    "winston": "^3.17.0",
    "zod": "^3.24.4",
    "zod-express-middleware": "^1.4.0"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/request-ip": "^0.0.41",
    "cross-env": "^10.0.0"
  }
